                <xsd:documentation>Сведения о Залогодателе (Эмитенте)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationPledgee" type="BondObligationPledgeeType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Сведения о Залогодержателе</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationResolution" type="BondObligationResolutionType">
              <xsd:annotation>
                <xsd:documentation>Сведения о решении о выпуске облигаций</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationAttorney" type="BondObligationAttorneyType">
              <xsd:annotation>
                <xsd:documentation>Сведения о заявителе, уполномоченном представителе Залогодателя (Эмитента)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FormUI2">
        <xsd:annotation>
          <xsd:documentation>об изменении залога (облигации)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CreationReferenceNumber" type="CreationReferenceNumberType">
              <xsd:annotation>
                <xsd:documentation>Регистрационный номер уведомления о возникновении залога</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationProperties" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Предметы залога по облигациям с залоговым обеспечением</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="BondObligationProperty" type="BondObligationPropertyType" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Предмет залога по облигациям с залоговым обеспечением</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="BondObligationPledgor" type="OrganizationType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Сведения о Залогодателе (Эмитенте)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationPledgee" type="BondObligationPledgeeType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Сведения о Залогодержателе(ях)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationResolution" type="BondObligationResolutionType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Сведения о решении о выпуске облигаций</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationAttorney" type="BondObligationAttorneyType">
              <xsd:annotation>
                <xsd:documentation>Сведения о заявителе, уполномоченном представителе Залогодателя (Эмитента)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FormUP2">
        <xsd:annotation>
          <xsd:documentation>о прекращении залога (облигации)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CreationReferenceNumber" type="CreationReferenceNumberType">
              <xsd:annotation>
                <xsd:documentation>Регистрационный номер уведомления о возникновении залога</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="BondObligationAttorney" type="BondObligationAttorneyType">
              <xsd:annotation>
                <xsd:documentation>Сведения о заявителе, уполномоченном представителе Залогодателя (Эмитента)</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
    <xsd:attribute name="version" type="xsd:string" use="required" fixed="2.0"/>
  </xsd:complexType>
  <xsd:complexType name="PledgeNotificationToNotaryType">
    <xsd:annotation>
      <xsd:documentation>Тип Уведомление, подаваемое нотариусу 2.0</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="BankData" minOccurs="0"/>
      <xsd:element name="NotificationNotaryAddress" type="PledgeNotificationNotaryAddressType">
        <xsd:annotation>
          <xsd:documentation>Данные нотариуса - адресата уведомления</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotificationData" type="PledgeNotificationDataType">
        <xsd:annotation>
          <xsd:documentation>Основные данные уведомления</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="version" type="xsd:string" use="required" fixed="2.0"/>
  </xsd:complexType>
  <!--================================ типы для Свидетельства о регистрации уведомления о залоге-->
  <xsd:complexType name="RegistrationCertificateDataType">
    <xsd:annotation>
      <xsd:documentation>Тип Основные данные свидетельства о регистрации уведомления 2.0</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="NotificationReferenceNumber" type="NotificationReferenceNumberType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Регистрационный номер</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RegistrationTime" type="DateTimeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Дата и время регистрации</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotificationData" type="PledgeNotificationDataType">
        <xsd:annotation>
          <xsd:documentation>Основные данные уведомления, на основании которого выдан сертификат</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RejectReason" type="String1-4000Type" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Причина отказа в выдаче свидетельства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="version" type="xsd:string" use="required" fixed="2.0"/>
  </xsd:complexType>
  <xsd:complexType name="RegistrationCertificateNotaryDataType">
    <xsd:annotation>
      <xsd:documentation>Тип Данные нотариуса для свидетельства о регистрации уведомления</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="IssuePlace" type="String1-255Type" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Место выдачи свидетельства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssueDate" type="DateType">
        <xsd:annotation>
          <xsd:documentation>Дата выдачи свидетельства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotaryName" type="String1-255Type">
        <xsd:annotation>
          <xsd:documentation>ФИО выдавшего свидетельство (нотариуса или врио)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotaryJobTitle" type="String1-255Type">
        <xsd:annotation>
          <xsd:documentation>Должность выдавшего свидетельство, включая округ (нотариуса или врио)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotaryJobTitleShort" type="String1-255Type">
        <xsd:annotation>
          <xsd:documentation>Должность выдавшего свидетельство (нотариуса или врио)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotaryID" type="String8Type">
        <xsd:annotation>
          <xsd:documentation>ID нотариуса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SignerID" type="String8or12Type">
        <xsd:annotation>
          <xsd:documentation>ID подписавшего документ - (нотариуса или врио)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RegistryNumber" type="String1-25Type" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Реестровый номер свидетельства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Date_Prikaz" type="DateType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Дата приказа органа юстиций о назначении на должность нотариуса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Num_Prikaz" type="String1-16Type" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Номер приказа органа юстиций о назначении на должность нотариуса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DateOfReceiving" type="DateType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Дата поступления уведомления</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Tariff" type="MoneyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Взыскано госпошлины (по тарифу)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Recovered" type="MoneyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Взыскано за техническую и правовую работу</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="RegistrationCertificateType">
    <xsd:annotation>
      <xsd:documentation>Тип Свидетельство о регистрации уведомления 2.0</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="BankData" minOccurs="0"/>
      <xsd:element name="RegistrationCertificateData" type="RegistrationCertificateDataType">
        <xsd:annotation>
          <xsd:documentation>Основные данные свидетельства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RegistrationCertificateNotaryData" type="RegistrationCertificateNotaryDataType">
        <xsd:annotation>
          <xsd:documentation>Данные свидетельства, заполняемые нотариусом</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="version" type="xsd:string" use="required" fixed="2.0"/>
  </xsd:complexType>
  <!--================================-->
  <xsd:element name="BankData">
    <xsd:annotation>
      <xsd:documentation>Данные Банка. Контейнер</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="UUID" type="xsd:token">
          <xsd:annotation>
            <xsd:documentation>Уникальный идентификатор документа в ИС Банка</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!--================================ XML документы-->
  <xsd:element name="PledgeNotificationToNotary" type="PledgeNotificationToNotaryType">
    <xsd:annotation>
      <xsd:documentation>Уведомление о залоге движимого имущества, подаваемое нотариусу</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
